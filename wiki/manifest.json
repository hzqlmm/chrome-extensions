{
  "name": "better experience for wikipedia",
  "version": "1.1",
  "permissions": [
    "tabs",
    "http://en.wikipedia.org/wiki/*"
  ],
 "background": {"scripts": ["background.js"] },
 "web_accessible_resources": ["background.jpg","loading-big.gif","header-bg.png","menu.png","menu_editor.png"],
 "content_scripts": [
    {
      "matches": ["http://en.wikipedia.org/wiki/*"],
      "js": ["jquery-1.7.2.min.js", 
      "jquery.extend.js",
      "jquery.easing.1.3.js",
      "jquery.mousewheel.min.js",
      "jquery-ui.min.js",
      "jquery.mCustomScrollbar.js",
      "jstorage.js",
      "document_start.js"
      ],
      "css":["base--theme-2.css","jquery.mCustomScrollbar.css","bulk.css"],
      "run_at":"document_start"
    }
  ],
  "browser_action": {
      "name": "better wiki experience",
      "default_icon":"icon.png"
  },
  "manifest_version": 2
}
